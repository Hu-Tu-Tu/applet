.order_item {padding: 20rpx;border-bottom: 1rpx solid #ccc;color: #666;}
.order_item .order_main {display: flex;justify-content: space-between;}
.order_main .num {display: flex;justify-content: space-between;padding: 10rpx 0;}
.order_main .price {display: flex;justify-content: space-between;padding: 10rpx 0;}
.price .price_con {color: var(--themeColor);font-size: 32rpx;}
.order_main .time {display: flex;justify-content: space-between;padding: 10rpx 0;}