<view class="tabs">
    <view class="title">
        <view class="title_item {{item.isActive?'active':''}}" wx:for="{{tabs}}" wx:key="*this" bindtap="handleItemTap"data-index="{{index}}">{{item.value}}</view>
    </view>
    <view class="content container">
        <slot></slot>
    </view>
</view>
